{
  "name": "restapi-mysql-express-js-es",
  "version": "1.0.0",
  "description": "API Rest, using micro framework Express and ECMAScript with connection to MySQL using the ORM Sequelize, with JWT authentication.",
  "main": "index.js",
  "scripts": {
    "build": "npx webpack --config webpack.config.js",
    "build-babel": "npx babel index.js --out-file dist/bundle.js",
    "build-babel-dir": "npx babel index.js --out-dir dist",
    "watch": "npx webpack --watch --config webpack.config.js",
    "prod": "node dist/bundle.js",
    "start": "npx babel-node index.js",
    "dev": "npx nodemon --exec babel-node index.js"
  },
  "keywords": [],
  "author": "Keisi Edward Flores.",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.2",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/cli": "^7.24.1",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.1",
    "babel-loader": "^9.1.3",
    "nodemon": "^3.1.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}